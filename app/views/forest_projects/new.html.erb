<h1>Create a new Project</h1>

<%= form_with model: @forest_project, local: true do |form| %>
  <div>
    <%= form.label :client_name %>
    <%= form.text_field :client_name %>
  </div>

  <div>
    <%= form.label :project_name %>
    <%= form.text_field :project_name %>
  </div>

  <div>
    <%= form.label :csv, "Upload CSV" %>
    <%= form.file_field :csv %>
  </div>

  <%= form.submit "Create Project" %>
<% end %>

<%= link_to 'Back', forest_projects_path %>